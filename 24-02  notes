package HI;

class ramki{
	void add() {
		System.out.println("ramki add");
	}
}
public class bli extends ramki {
	void add() {
		super.add();
		System.out.println("bli add");
	}
public static void main(String[] args) {
	ramki obj=new bli();
	obj.add();
}
}
//cons
