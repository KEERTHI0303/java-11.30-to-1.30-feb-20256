}
}

package HI;

class demo{
	private String course;

	/*void live(String course) {
		this.course=course;
	}*/
	demo(String course){
		this.course=course;
	}
	void wire() {
		System.out.println(course);
	}
}
public class gh {
    public static void main(String[] args) {
		demo obj=new demo("java");
		obj.wire();
	}
}

package HI;

class karthi{
	public String name="karthi";// within package 
	protected String phoneno="8989898989";// subclass use
	private int age=24;//within class
	void details() {
		System.out.println(name);
		System.out.println(phoneno);
		System.out.println(age);
	}
	public int getAge() {
		return age;
	}
	public void setAge(int age) {
		this.age = age;
	}
}
public class n extends karthi{
	void details23() {
		System.out.println(name);
		System.out.println(phoneno);
		System.out.println(getAge());
	}
public static void main(String[] args) {
	n obj=new n();
	obj.details();
	obj.setAge(90);
	obj.details23();
}
}


package HI;

public class koko {
	
	koko(){
		System.out.println("normal cons");
	}
	koko(koko obj){
		
		System.out.println("copy cons");
	}
public static void main(String[] args) {
 koko obj=	new koko();//cons
	new koko(obj);//copy cons
}
}


