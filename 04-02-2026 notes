package abc;

public class arr {
public static void main(String[] args) {
	int a[]= {11,12,13,14,15};
	for(int i=0;i<6;i++) {
		try {
			System.out.println(a[i]);
		}
		catch(Exception e){
			System.out.println(e);
		}
		
	}
}
}

// fibno

      package abc;

public class fac {
	int fib(int num) {
		if(num<0) {
			System.out.println("invalid");
		}
		else if(num==0 || num==1) {
			return num;
		}
		else if(num>1) {
			return (fib(num-2)+fib(num-1));
		}
		return 0;
	}
	    
 
	public static void main(String[] args) {
		fac obj=new fac();
		System.out.println(obj.fib(8));
	}

}



